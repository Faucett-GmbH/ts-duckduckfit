{
	"name": "ts-duckduckfit",
	"private": true,
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"svelte-kit": "svelte-kit",
		"tauri": "tauri",
		"dev": "vite dev",
		"devo": "vite dev --open",
		"build": "paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide && vite build",
		"preview": "vite preview",
		"desktop": "tauri dev",
		"desktop:build": "tauri build",
		"android": "npm run tauri android dev --open",
		"android:build": "npm run tauri android build",
		"ios": "npm run tauri ios dev --open",
		"ios:build": "npm run tauri ios build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"postinstall": "svelte-kit sync && paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide"
	},
	"devDependencies": {
		"@eslint/compat": "^1.3.1",
		"@eslint/js": "^9.31.0",
		"@inlang/cli": "^3.0.12",
		"@inlang/paraglide-js": "2.2.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.22.5",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/vite": "4.1.11",
		"@tauri-apps/cli": "^2.6.2",
		"@types/clone-deep": "^4.0.4",
		"@types/qrcode": "^1.5.5",
		"autoprefixer": "^10.4.21",
		"eslint": "^9.31.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-svelte": "^3.10.1",
		"globals": "^16.3.0",
		"jsdom": "^26.1.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.35.6",
		"svelte-check": "^4.2.2",
		"tailwindcss": "^4.1.11",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.36.0",
		"vite": "^7.0.4",
		"vite-plugin-devtools-json": "^0.2.1",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"@aicacia/debounce": "^0.1.25",
		"@aicacia/keepalivewebsocket": "^0.1.2",
		"@aicacia/peer": "^0.1.5",
		"@aicacia/string-fuzzy_equals": "^0.1.4",
		"@automerge/automerge": "^2.2.9",
		"@automerge/automerge-repo": "^2.0.8",
		"@automerge/automerge-repo-storage-indexeddb": "^2.0.8",
		"@fingerprintjs/fingerprintjs": "^4.6.2",
		"@svelte-plugins/datepicker": "^1.0.11",
		"@svelte-put/clickoutside": "^4.0.0",
		"@zip.js/zip.js": "^2.7.63",
		"cbor-x": "^1.6.0",
		"clone-deep": "^4.0.1",
		"date-fns": "^4.1.0",
		"eventemitter3": "^5.0.1",
		"lucide-svelte": "^0.525.0",
		"qrcode": "^1.5.4",
		"svelte-portal": "^2.2.1",
		"uuid": "^11.1.0",
		"vest": "^5.4.6"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@nestjs/core",
			"@sveltejs/kit",
			"@swc/core",
			"@tailwindcss/oxide",
			"cbor-extract",
			"esbuild"
		]
	}
}