{
	"name": "ts-duckduckfit",
	"private": true,
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"tauri": "tauri",
		"dev": "vite dev",
		"build": "paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide && vite build",
		"preview": "vite preview",
		"desktop": "tauri dev",
		"desktop:build": "tauri build",
		"android": "npm run tauri android dev --open",
		"android:build": "npm run tauri android build",
		"ios": "npm run tauri ios dev --open",
		"ios:build": "npm run tauri ios build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"postinstall": "paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide",
		"exdb-openapi-local": "rm -rf src/lib/openapi/exdb && openapi-generator-cli generate -i http://localhost:8000/schema/openapi.json -g typescript-fetch -o 'src/lib/openapi/exdb' --type-mappings=DateTime=Date --additional-properties=supportsES6=true,withInterfaces=true,useSingleRequestParameter=false"
	},
	"devDependencies": {
		"@aicacia/hash": "^0.2.3",
		"@eslint/compat": "^1.2.7",
		"@eslint/js": "^9.23.0",
		"@inlang/cli": "^3.0.7",
		"@inlang/paraglide-js": "2.0.4",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.20.2",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/vite": "4.0.15",
		"@tauri-apps/cli": "^2.4.0",
		"@types/qrcode": "^1.5.5",
		"autoprefixer": "^10.4.21",
		"eslint": "^9.23.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-svelte": "^3.3.3",
		"globals": "^16.0.0",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.25.2",
		"svelte-check": "^4.1.5",
		"tailwindcss": "^4.0.15",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.27.0",
		"vite": "^6.2.2",
		"vitest": "^3.0.9"
	},
	"dependencies": {
		"@aicacia/debounce": "^0.1.23",
		"@automerge/automerge": "^2.2.8",
		"@automerge/automerge-repo": "^1.2.1",
		"@automerge/automerge-repo-network-websocket": "^1.2.1",
		"@automerge/automerge-repo-storage-indexeddb": "^1.2.1",
		"@svelte-put/clickoutside": "^4.0.0",
		"eventemitter3": "^5.0.1",
		"lucide-svelte": "^0.483.0",
		"qrcode": "^1.5.4",
		"svelte-localstorage-writable": "^0.1.3",
		"svelte-portal": "^2.2.1",
		"vest": "^5.4.6"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@sveltejs/kit",
			"@swc/core",
			"cbor-extract",
			"esbuild"
		]
	}
}